{
    "name": "LEIRA Technologies, LLC./TrackZero",
    "type": "library",
    "description": "TZ Desc Here",
    "keywords": [
        "LEIRA Technologies, LLC.",
        "TrackZero"
    ],
    "homepage": "https://github.com/LEIRA Technologies, LLC./TrackZero",
    "license": "MIT",
    "authors": [
        {
            "name": "Adel Alzubeir",
            "email": "publisher@trackzero.io",
            "homepage": "https://www.trackzero.io",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "~7.2"
    },
    "require-dev": {
        "phpunit/phpunit" : ">=8.0",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "leira\\trackzero\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "leira\\trackzero\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
